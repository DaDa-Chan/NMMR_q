{
  "data_configs": {
    "sgd": {
      "data_path": "/Users/chen/Study/CI/NMMR_q/data/SGD"
    },
    "rhc": {
      "data_path": "/Users/chen/Study/CI/NMMR_q/data/right_heart_catheterization",
      "use_all_X": false
    }
  },
  "train_params": {
    "model_name": "nmmr_q",
    "loss_name": "U_statistic",
    "n_epochs": 100,
    "batch_size": 200,
    "n_splits":5,
    "log_metrics": "True",
    "network_depth": 4,
    "use_u_statistic": false,
    "kernel_gamma": 1.0,
